{
    "name": "thingst-web",
    "version": "0.0.0",
    "description": "A Little Store of Things âœ¨",
    "scripts": {
        "prepare": "yarn husky install",
        "prettier": "npx prettier --write . --config ./config/.prettierrc --ignore-path ./config/.prettierignore",
        "dev": "vite --config ./config/vite.config.ts",
        "prebuild": "tsc -p ./config/tsconfig.json",
        "build": "vite build --config ./config/vite.config.ts",
        "preview": "vite preview --config ./config/vite.config.ts --port 8080",
        "start": "yarn build && yarn preview"
    },
    "dependencies": {
        "react": "18.0.0",
        "react-dom": "18.0.0"
    },
    "devDependencies": {
        "@commitlint/cli": "16.2.3",
        "@commitlint/config-conventional": "16.2.1",
        "@types/react": "18.0.0",
        "@types/react-dom": "18.0.0",
        "@vitejs/plugin-react": "1.3.0",
        "husky": "7.0.4",
        "prettier": "2.6.2",
        "typescript": "4.6.3",
        "vite": "2.9.2"
    },
    "repository": "git@github.com/roremdev/thingst-web",
